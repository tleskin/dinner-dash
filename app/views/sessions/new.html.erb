
<h1>Login to HipsterBytes</h1>
<div id="login_form">

  <%= form_for(:session, url: login_path) do |f| %>
  <div class="field"><%= f.text_field :email, placeholder: 'Email' %><br></div>
  <div class="field"><%= f.password_field :password, placeholder: 'Password' %><br></div>
  <div class="submit"><%= f.submit 'Submit'%></div>
  <% end %>
</div>

  <div class="twitter-login">
    <%= link_to "Login with Twitter", login_twitter_path %>
  </div>
